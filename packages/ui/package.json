{
	"name": "@mrunner/ui",
	"version": "0.0.1",
	"private": true,
	"type": "module",
	"sideEffects": false,
	"main": "./src/index.ts",
	"types": "./src/index.ts",
	"exports": {
		".": {
			"types": "./src/index.ts",
			"import": "./src/index.ts"
		},
		"./components/*": {
			"types": "./src/components/ui/*.tsx",
			"import": "./src/components/ui/*.tsx"
		},
		"./lib/*": {
			"types": "./src/lib/*.ts",
			"import": "./src/lib/*.ts"
		}
	},
	"dependencies": {
		"@base-ui/react": "^1.0.0",
		"@hugeicons/core-free-icons": "^3.1.0",
		"@hugeicons/react": "^1.1.3",
		"class-variance-authority": "^0.7.1",
		"clsx": "^2.1.1",
		"cmdk": "^1.1.1",
		"lucide-react": "^0.562.0",
		"tailwind-merge": "^3.4.0"
	},
	"peerDependencies": {
		"react": "^19",
		"react-dom": "^19"
	},
	"devDependencies": {
		"@types/react": "^19",
		"@types/react-dom": "^19",
		"typescript": "^5"
	}
}
